    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" prefHeight="18.0" prefWidth="130.0" textFill="#c92424" />
    <Button onAction="#onHelloButtonClick" text="Hello, Bella!" textFill="#cc1111" />
</VBox>
